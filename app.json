      "bundleIdentifier": "com.spacefarmers.chiapool",
      "buildNumber": "0.0.9",
      "supportsTablet": true
    },
    "android": {
      "package": "com.spacefarmers.chiapool",
      "versionCode": 18,
      "permissions": ["android.permission.RECEIVE_BOOT_COMPLETED", "android.permission.SCHEDULE_EXACT_ALARM"],
      "googleServicesFile": "./google-services.json",
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#000000"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "0e9318fe-986e-41a6-a5b8-dbf7d16ff8de"
      }
    }
  }
